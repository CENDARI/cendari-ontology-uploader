<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
        <servlet-name>DatasetDescriptionUpload</servlet-name>
        <servlet-class>org.cendari.ontology.upload.DatasetDescriptionUpload</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DatasetDescriptionUpload</servlet-name>
        <url-pattern>/RecvDatasetDescription.do</url-pattern>
    </servlet-mapping>
     
    <servlet>
        <servlet-name>DatasetDescriptionUploadIgnoreWarning</servlet-name>
        <servlet-class>org.cendari.ontology.upload.DatasetDescriptionUploadIgnoreWarning</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DatasetDescriptionUploadIgnoreWarning</servlet-name>
        <url-pattern>/RecvDatasetDescriptionIgnoreWarning.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>OriginalFilesUpload</servlet-name>
        <servlet-class>org.cendari.ontology.upload.OriginalFilesUpload</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>OriginalFilesUpload</servlet-name>
        <url-pattern>/RecvOriginalFiles.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TransformedFilesUpload</servlet-name>
        <servlet-class>org.cendari.ontology.upload.TransformedFilesUpload</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>TransformedFilesUpload</servlet-name>
        <url-pattern>/RecvTransformedFiles.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>RelationshipFilesUpload</servlet-name>
        <servlet-class>org.cendari.ontology.upload.RelationshipFilesUpload</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>RelationshipFilesUpload</servlet-name>
        <url-pattern>/RecvRelationshipFiles.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AdditionalFilesUpload</servlet-name>
        <servlet-class>org.cendari.ontology.upload.AdditionalFilesUpload</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>AdditionalFilesUpload</servlet-name>
        <url-pattern>/RecvAdditionalFiles.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AllFilesUpload</servlet-name>
        <servlet-class>org.cendari.ontology.upload.RelationshipFileUpload</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>AllFilesUpload</servlet-name>
        <url-pattern>/RecvAllFiles.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>NextStep</servlet-name>
        <servlet-class>org.cendari.ontology.utility.NextStep</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>NextStep</servlet-name>
        <url-pattern>/NextStep</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DataspaceIdReceived</servlet-name>
        <servlet-class>org.cendari.ontology.download.DataspaceIdReceived</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>DataspaceIdReceived</servlet-name>
        <url-pattern>/RecvDataspaceId.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DatasetsList</servlet-name>
        <servlet-class>org.cendari.ontology.download.DatasetsList</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DatasetsList</servlet-name>
        <url-pattern>/ListDatasets.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ResourcesList</servlet-name>
        <servlet-class>org.cendari.ontology.download.ResourcesList</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ResourcesList</servlet-name>
        <url-pattern>/ListResources.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>FileDownload</servlet-name>
        <servlet-class>org.cendari.ontology.download.FileDownload</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FileDownload</servlet-name>
        <url-pattern>/DownloadFile.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>FileDelete</servlet-name>
        <servlet-class>org.cendari.ontology.delete.FileDelete</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FileDelete</servlet-name>
        <url-pattern>/DeleteFile.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DisplayHeaders</servlet-name>
        <servlet-class>test.DisplayRequestHeaders</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DisplayHeaders</servlet-name>
        <url-pattern>/DisplayHeaders.do</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
    	<welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
